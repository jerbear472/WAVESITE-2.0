    "jobs:predict": "node -r dotenv/config -e 'require(\"./lib/backgroundJobs\").backgroundJobs.predictTrends()'",
    "jobs:all": "node -r dotenv/config -e 'require(\"./lib/backgroundJobs\").backgroundJobs.runAllJobs()'"
